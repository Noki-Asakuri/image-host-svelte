{
    "name": "image-host-svelte",
    "version": "0.0.1",
    "scripts": {
        "dev": "vite dev --port 3000 --host",
        "build": "vite build",
        "preview": "vite preview --port 3000 --host",
        "build/start": "vite build && pnpm preview",
        "package": "svelte-kit package",
        "check": "svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
        "lint": "prettier --check --plugin-search-dir=. . && eslint .",
        "format": "prettier --write --plugin-search-dir=. .",
        "postinstall": "prisma generate"
    },
    "devDependencies": {
        "@sveltejs/adapter-auto": "next",
        "@sveltejs/kit": "next",
        "@types/sharp": "^0.30.5",
        "@typescript-eslint/eslint-plugin": "^5.36.2",
        "@typescript-eslint/parser": "^5.36.2",
        "autoprefixer": "^10.4.8",
        "eslint": "^8.23.0",
        "eslint-plugin-svelte3": "^4.0.0",
        "postcss": "^8.4.31",
        "prettier": "^2.7.1",
        "prettier-plugin-tailwindcss": "^0.1.13",
        "prisma": "^4.3.1",
        "svelte": "^3.50.1",
        "svelte-check": "^2.9.0",
        "svelte-french-toast": "^1.0.3",
        "svelte-portal": "^2.2.0",
        "svelte-preprocess": "^4.10.7",
        "tailwindcss": "^3.1.8",
        "tslib": "^2.4.0",
        "typescript": "^4.8.3",
        "vite": "^3.1.0"
    },
    "type": "module",
    "dependencies": {
        "@prisma/client": "^4.3.1",
        "@supabase/storage-js": "^1.7.3",
        "@trpc/client": "10.0.0-proxy-alpha.74",
        "@trpc/server": "10.0.0-proxy-alpha.74",
        "sharp": "^0.31.0",
        "trpc-sveltekit": "^2.3.4",
        "zod": "^3.19.0"
    }
}
