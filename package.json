{
    "name": "image-host-svelte",
    "version": "0.0.1",
    "scripts": {
        "dev": "vite dev --port 3000 --host",
        "build": "vite build",
        "preview": "vite preview --port 3000 --host",
        "build/start": "pnpm build && pnpm preview",
        "package": "svelte-kit package",
        "check": "svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
        "lint": "prettier --check --plugin-search-dir=. . && eslint .",
        "format": "prettier --write --plugin-search-dir=. .",
        "postinstall": "prisma generate"
    },
    "devDependencies": {
        "@sveltejs/adapter-auto": "1.0.0-next.66",
        "@sveltejs/kit": "1.0.0-next.437",
        "@types/sharp": "^0.30.5",
        "@typescript-eslint/eslint-plugin": "^5.35.1",
        "@typescript-eslint/parser": "^5.35.1",
        "autoprefixer": "^10.4.8",
        "eslint": "^8.22.0",
        "eslint-plugin-svelte3": "^4.0.0",
        "postcss": "^8.4.16",
        "prettier": "^2.7.1",
        "prettier-plugin-tailwindcss": "^0.1.13",
        "prisma": "^4.2.1",
        "svelte": "^3.49.0",
        "svelte-check": "^2.8.1",
        "svelte-french-toast": "^1.0.3",
        "svelte-preprocess": "^4.10.7",
        "tailwindcss": "^3.1.8",
        "tslib": "^2.4.0",
        "typescript": "^4.7.4",
        "vite": "^3.0.9"
    },
    "type": "module",
    "dependencies": {
        "@prisma/client": "^4.2.1",
        "@supabase/storage-js": "^1.7.3",
        "@trpc/client": "^9.27.2",
        "@trpc/server": "^9.27.2",
        "sharp": "^0.30.7",
        "trpc-sveltekit": "^2.3.0",
        "zod": "^3.18.0"
    }
}
